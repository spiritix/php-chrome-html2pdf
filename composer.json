{
    "name": "spiritix/php-chrome-html2pdf",
    "type": "library",
    "description": "A PHP library for converting HTML to PDF using Google Chrome",
    "license": "MIT",
    "keywords": ["php","html","pdf","Html2Pdf","html2pdf","chrome","headless","puppeteer"],
    "authors": [
        {
            "name": "Matthias Isler",
            "email": "mi@matthias-isler.ch"
        }
    ],
    "require": {
        "php": ">=7.0",
        "eloquent/composer-npm-bridge": "^4|^5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Spiritix\\Html2Pdf\\": "src/Spiritix/Html2Pdf/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiritix\\Html2Pdf\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "npm-bridge": {
            "timeout": 3600
        }
    }
}
